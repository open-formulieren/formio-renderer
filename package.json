{
  "name": "@open-formulieren/formio-renderer",
  "version": "0.1.0",
  "description": "Alternative Form.io renderer made for Open Forms.",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    },
    "./esm/": "./lib/esm/",
    "./lib/": "./lib/"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "build:esm": "tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "build:storybook": "storybook build --webpack-stats-json",
    "build": "npm-run-all build:*",
    "test": "npm-run-all test:*",
    "test:storybook": "test-storybook",
    "format": "prettier --write 'src/**/*'",
    "checkformat": "prettier --check 'src/**/*'",
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "makemessages": "formatjs extract",
    "compilemessages": "formatjs compile-folder --ast i18n/messages i18n/compiled",
    "watch": "tsc -p tsconfig.prod.json --watch"
  },
  "files": [
    "lib/",
    "i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/open-formulieren/formio-renderer.git"
  },
  "author": "Maykin Media",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/open-formulieren/formio-renderer/issues"
  },
  "homepage": "https://github.com/open-formulieren/formio-renderer#readme",
  "peerDependencies": {
    "@utrecht/component-library-react": "^1.0.0-alpha.366",
    "formiojs": "~4.13.0 || ~4.14.0",
    "react": "^18.0.0",
    "react-intl": "^6.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@formatjs/cli": "^6.1.5",
    "@formatjs/ts-transformer": "^3.13.5",
    "@open-formulieren/design-tokens": "^0.39.0",
    "@open-formulieren/types": "^0.9.0",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/client-api": "^7.3.2",
    "@storybook/jest": "^0.2.1",
    "@storybook/react": "^7.3.2",
    "@storybook/react-webpack5": "^7.3.2",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "^18.0.33",
    "@types/webpack": "^5.28.1",
    "@utrecht/component-library-css": "1.0.0-alpha.557",
    "@utrecht/component-library-react": "1.0.0-alpha.366",
    "babel-loader": "^8.3.0",
    "babel-plugin-formatjs": "^10.5.5",
    "formiojs": "~4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-formio": "^4.3.0",
    "react-intl": "^6.4.6",
    "react-vega": "^7.6.0",
    "storybook": "^7.3.2",
    "storybook-react-intl": "^2.0.2",
    "ts-patch": "^2.1.0",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4.9.5",
    "vega": "^5.25.0",
    "vega-lite": "^5.15.0"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "formik": "^2.2.9",
    "react-select": "^5.7.2",
    "zod": "^3.21.4",
    "zod-formik-adapter": "^1.2.0"
  },
  "resolutions": {
    "react-formio/formiojs": "~4.13.0"
  }
}
